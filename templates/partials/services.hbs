 <!-- services Grid-->
<section class="page-section" id="services">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">services</h2>
            <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet
                consectetur.</h3>
        </div>
        <div class="row">

            {{#each services}}
            <div class="col-lg-3 col-sm-6 mb-4">
                <div class="services-item">
                    <div class="stitches-purple">
                        <a class="services-link" data-toggle="modal"
                            href="#servicesModal1">

                            <img class="img-fluid" src="{{img}}" alt="{{alt}}"
                                />
                        </a>
                    </div>
                    <div class="services-caption">
                        <div class="services-caption-heading">{{title}}</div>
                        <div class="{{desc}}">Long-Arm Services</div>
                    </div>
                </div>
            </div>
            {{/each}}



        </div>

    </div>
</section>
